package paquete;

import java.awt.*;

import javax.swing.*;

public class Panel extends JPanel {
	
	public Image fondo = new ImageIcon("liga.png").getImage();
	private final int PANEL_ALTO = 450;
	private final int PANEL_ANCHO = 300;

	public Panel() {
		this.setLayout(null);
		this.setSize(PANEL_ANCHO, PANEL_ALTO);
	}
	
	
	@Override
	public void paint(Graphics g) {
		super.paint(g);
		g.drawImage(fondo, 0, 0, null);
	}
	
}
